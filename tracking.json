{
    "trackingNumber": "1Z{{chance.string({ length: 6 })}}{{chance.integer({ min: 10, max: 99 })}}{{chance.integer({ min: 10000000, max: 99999999 })}}",
    "packageRFID": "{{chance.string({ length: 13 })}}{{chance.string({ length: 12 })}}",
    "activity": {
        "$choose": {
            "from": [
                {
                    "timestamp": {
                        "$date": {
                            "min": "2022-01-01",
                            "max": "2022-03-31"
                        }
                    },
                    "location": "$city",
                    "gps_location": "{{chance.coordinates()}}",
                    "public_event": {
                        "$choose": {
                            "from": [
                                "origin scan",
                                "departure scan",
                                " export scan",
                                "customs entry",
                                "customs exit",
                                "import scan",
                                "destination scan",
                                "out on delivery",
                                "delivered"
                            ]
                        }
                    }
                },
                {
                    "timestamp": {
                        "$date": {
                            "min": "2022-01-01",
                            "max": "2022-03-31"
                        }
                    },
                    "location": "$city",
                    "gps_location": "{{chance.coordinates()}}",
                    "private_event": {
                        "$choose": {
                            "from": [
                                "loading",
                                "unloading"
                            ]
                        }
                    }
                }
            ]
        }
    }
}